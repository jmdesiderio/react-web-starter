{
  "name": "react-web-starter",
  "version": "0.0.1",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/jmdesiderio/react-web-starter.git"
  },
  "engines": {
    "npm": ">=4",
    "node": ">=7"
  },
  "author": "Jason Desiderio",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze": "node ./internals/scripts/analyze.js",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:js",
    "postinstall": "npm run build:dll",
    "preanalyze": "npm run analyze:clean",
    "prebuild": "npm run build:clean",
    "pretest": "npm run test:clean && npm run lint",
    "start:prod": "NODE_ENV=production node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:tunnel": "NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "NODE_ENV=development node server",
    "test:clean": "rimraf ./coverage",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test": "NODE_ENV=test jest --coverage"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.js",
      "!app/**/*.test.js",
      "!app/*/RbGenerated*/*.js",
      "!app/index.js",
      "!app/routes.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": ".*\\.test\\.js$"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.14.1",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "ip": "1.1.4",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-intl": "2.2.3",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "styled-components": "1.4.2",
    "whatwg-fetch": "2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.22.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "2.0.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-redux-saga": "0.3.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.7.0",
    "jest-cli": "18.1.0",
    "lint-staged": "3.3.0",
    "ngrok": "2.2.6",
    "null-loader": "0.1.1",
    "offline-plugin": "4.5.5",
    "pre-commit": "1.2.2",
    "react-addons-test-utils": "15.4.2",
    "rimraf": "2.5.4",
    "shelljs": "0.7.6",
    "sinon": "2.0.0-pre",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.16.1"
  }
}
